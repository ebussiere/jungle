<tr>
  <td>
    <%= image_tag product.image.tiny %>
  </td>
  <td>
    <h4><%= product.name %></h4>
    <br>
    <h4><%= product.description %></h4>
  </td>
  <td>
    <%= number_to_currency(product.price) %>
  </td>
  <td>
    <%=quantity%>
  </td>
  <td>
    <%= number_to_currency(product.price * quantity) %>
  </td>
</tr>